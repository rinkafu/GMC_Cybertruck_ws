<launch>
	<!-- master auto="start" -->

	<!-- param name ="use_sim_time" value="false" -->

	<!--- Run the map server --> 

	<node  pkg="map_server" type="map_server" name="map_server"  args="/home/cybertruck/Maps/Map27.yaml"/>
	<include file="$(find amcl)/examples/amcl_diff.launch"/>

	<!--- Run move base --> 

	<node pkg="move_base" type="move_base" name="move_base" respawn="false" output="screen">
	
	<param name="base_global_planner" value="carrot_planner/CarrotPlanner"/>


	<rosparam file="src/navigation/conf/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
	<rosparam file="src/navigation/conf/costmap_common_params.yaml" command="load" ns="local_costmap" />
	<rosparam file="src/navigation/conf/local_costmap_params.yaml" command="load" /> 
	<rosparam file="src/navigation/conf/global_costmap_params.yaml" command="load" />
	<rosparam file="src/navigation/conf/move_base_params.yaml" command="load" />
	<rosparam file="src/navigation/conf/base_local_planner_params.yaml" command="load" /> 
	</node>

</launch>