<?xml version="1.0"?>
<launch>
    <node name="motor" pkg="motor_ctl" type="motor_ctl.py"  output="screen" respawn="true" respawn_delay="1" />
    <include file="$(find rplidar_ros)/launch/rplidar.launch" />
    <include file="$(find rf2o_laser_odometry)/launch/rf2o_laser_odometry.launch" />
    <node name="gmc_tf" pkg="gmc_tf" type="gmc_tf_node"  output="screen" respawn="true" respawn_delay="2" />
    <include file="$(find navigation)/launch/navigation_gmc.launch" />
    <node name="costmap" pkg="costmap_converter" type="standalone_converter"  output="log" respawn="true" respawn_delay="3"/>
    <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard" name="teleop_twist_keyboard.py" output="screen" respawn="true" respawn_delay="1">
        <param name="_speed" value="0.15"/>
        <param name="_turn" value="0.15"/>
        <param name="cmd_vel" value="pc_vel"/>
        <param name="_repeat_rate" value="10.0"/>
        <param name="_key_timeout" value="0.5"/>
    </node>
</launch>